{
  "name": "mofish-plugin-proxy",
  "version": "0.5.3",
  "description": "A plugin for mofish tool to proxy requests.",
  "main": "index.js",
  "private": false,
  "files": [
    "dist",
    "bin",
    "server-dist",
    "server",
    "LICENSE"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:all": "npm run build && npm run build:server",
    "lint": "vue-cli-service lint",
    "build:server": "babel server --presets env --plugins=babel-plugin-syntax-object-rest-spread,transform-runtime --out-dir server-dist",
    "test:unit": "vue-cli-service test:unit",
    "publish": "npm run build:all && npm publish",
    "publish:beta": "npm run build:all && npm publish --tag=beta"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "axios": "^0.19.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "core-js": "^2.6.5",
    "element-ui": "^2.11.1",
    "http-proxy-middleware": "^0.19.1",
    "koa": "^2.7.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-server-http-proxy": "^0.1.0",
    "koa-sslify": "^4.0.3",
    "koa-static": "^5.0.0",
    "koa2-connect": "^1.0.2",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "node-gzip": "^1.1.2",
    "vue": "^2.6.10",
    "vue-grid-layout": "^2.3.4",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-plugin-unit-mocha": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "fs-readdir-recursive": "^1.1.0",
    "node-sass": "^4.9.0",
    "output-file-sync": "^2.0.1",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
