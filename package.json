{
  "name": "mofish-plugin-proxy",
  "version": "0.7.2",
  "description": "A plugin for mofish tool to proxy requests and mock.",
  "main": "./dist-dev/dev.js",
  "private": false,
  "files": [
    "dist",
    "bin",
    "server-dist",
    "server",
    "LICENSE"
  ],
  "scripts": {
    "dev:electron": "nodemon --watch electron --watch server --exec \"npm run lint:electron && mkdir -p dist && babel server --presets env --plugins babel-plugin-syntax-object-rest-spread --out-dir ./dist-server && babel electron --presets env --plugins babel-plugin-syntax-object-rest-spread --out-dir ./dist-dev && ./node_modules/.bin/electron .\"",
    "dev:both": "npm run serve & npm run dev:electron",
    "dev:command": "cross-env mode=\"development\" webpack --config ./build/webpack.conf.command.js --mode development",
    "build:command": "cross-env mode=\"production\" webpack --config ./build/webpack.conf.command.js --mode production",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:electron": "electron-builder --win --x64",
    "build:both": "npm run build && npm run build:electron",
    "lint": "vue-cli-service lint",
    "lint:server": "eslint server",
    "lint:electron": "npm run lint:server && eslint electron",
    "test:unit": "vue-cli-service test:unit",
    "publish": "npm run build:all && npm publish",
    "publish:beta": "npm run build:all && npm publish --tag=beta"
  },
  "build": {
    "appId": "com.mofish_plugin_proxy.app",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "axios": "^0.19.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "commander": "^5.1.0",
    "core-js": "^2.6.5",
    "element-ui": "^2.11.1",
    "http-proxy-middleware": "^0.19.1",
    "koa": "^2.7.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-server-http-proxy": "^0.1.0",
    "koa-sslify": "^4.0.3",
    "koa-static": "^5.0.0",
    "koa2-connect": "^1.0.2",
    "lodash": "^4.17.15",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "md5": "^2.2.1",
    "mofish-common-tool": "^0.0.1",
    "node-gzip": "^1.1.2",
    "vue": "^2.6.10",
    "vue-grid-layout": "^2.3.4",
    "vue-router": "^3.0.3",
    "vue2-ace-editor": "^0.0.15",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-plugin-unit-mocha": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "electron": "^8.0.2",
    "electron-builder": "^22.3.2",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "fs-readdir-recursive": "^1.1.0",
    "node-sass": "^4.9.0",
    "output-file-sync": "^2.0.1",
    "run-node-webpack-plugin": "^1.2.2",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  }
}
